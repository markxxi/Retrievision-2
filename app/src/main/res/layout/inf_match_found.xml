<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:backgroundTint="#F1F5F9"
    tools:ignore="HardcodedText, UseCompatTextViewDrawableXml,UseCompoundDrawables,ContentDescription">
    <ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
        android:layout_marginBottom="70dp">
    <RelativeLayout
            android:id="@+id/matched_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:ignore="UselessParent"
            android:layout_margin="10dp">
        <LinearLayout
            android:id="@+id/dialog_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="-6dp"
            android:paddingHorizontal="4dp"
            android:visibility="gone">
            <com.google.android.material.chip.Chip
                android:id="@+id/report_type"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                app:chipIcon="@drawable/icon_box"
                android:textColor="@android:color/white"
                app:chipCornerRadius="8dp"
                app:chipIconSize="13dp"
                android:textSize="11sp"
                app:chipStrokeColor="@android:color/transparent"
                app:chipEndPadding="1dp"
                app:textStartPadding="5dp">
            </com.google.android.material.chip.Chip>

            <com.google.android.material.chip.Chip
                android:id="@+id/status_tag"
                android:layout_width="wrap_content"
                android:layout_height="41dp"
                android:layout_marginHorizontal="4dp"
                app:chipIcon="@drawable/shape_rectangle_40"
                app:chipIconSize="13dp"
                app:chipBackgroundColor="#DADEE2"
                app:chipCornerRadius="15dp"
                app:chipStrokeColor="@android:color/transparent"
                android:text="status"
                android:fontFamily="@font/poppinsmedium"
                android:textSize="11sp"
                app:chipEndPadding="2dp"
                app:textStartPadding="5dp">
            </com.google.android.material.chip.Chip>
            <View
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">
            </View>
            <ImageView
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:src="@drawable/shape_rectangle_40"
                app:tint="#BABDC1"
                android:layout_gravity="center_vertical">
            </ImageView>
        </LinearLayout>


        <androidx.cardview.widget.CardView
            android:layout_below="@id/dialog_header"
                android:id="@+id/cv"
                android:layout_width="match_parent"
                android:layout_height="170dp"
                android:layout_marginBottom="20dp"
                app:cardCornerRadius="20dp">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:contentDescription="@string/ContentDescription"
                    android:scaleType="centerCrop" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/cvtext"
                android:layout_gravity="center"
                android:text="image not available"
                android:textAlignment="center"
                android:visibility="gone">

            </TextView>
            </androidx.cardview.widget.CardView>
        <LinearLayout
            android:id="@+id/object_attributes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/cv"
            android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/categoryContainer"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="21dp"
                                android:layout_height="21dp"
                                android:padding="2dp"
                                android:src="@drawable/icon_category" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="13dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Category:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getCategoryAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="28.5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/typeContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/icon_box_stroke_confirmation"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Type:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getTypeAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="53dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/colorContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/icon_color" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Color:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getColorAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="51dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/brandContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Brand:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getBrandAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="48.5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/modelContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Model:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getModelAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="47.5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/sizeContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Size:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getSizeAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="60dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/shapeContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Shape:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getShapeAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="46dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dimensionContainer"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Dimension:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getWidthAsText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="22dp"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/getHeightAsText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/textContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Texts:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getTextAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="53dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/remarksContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="17dp"
                                android:layout_height="17dp"
                                android:padding="2dp"
                                android:layout_marginStart="2dp"
                                android:src="@drawable/icon_list" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Remarks:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getRemarksAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="32.5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/locationContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="3dp"
                                android:src="@drawable/icon_location" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="14dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Location:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/getLocationAsText"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="35dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/poppinsmedium"
                            android:text="placeholder"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dateContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:padding="2dp"
                                android:src="@drawable/icon_time"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="15dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="Date:"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:orientation="vertical"
                            android:layout_marginStart="55.5dp">

                            <TextView
                                android:id="@+id/getDateAsText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="placeholder"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/getTimeAsText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:fontFamily="@font/poppinsmedium"
                                android:text="placeholder"
                                android:textSize="12sp" />
                        </LinearLayout>
                    </LinearLayout>
        </LinearLayout>
        </RelativeLayout>
    </ScrollView>
    <LinearLayout
        android:id="@+id/ncbtn_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:layout_marginBottom="10dp">
        <com.google.android.material.button.MaterialButton
            android:id="@+id/notMine"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginStart="8dp"
            android:backgroundTint="@android:color/transparent"
            app:strokeWidth="1.3dp"
            app:strokeColor="@color/blue"
            app:cornerRadius="10dp"
            android:text="NOT MINE"
            android:fontFamily="@font/poppinsmedium"
            android:textColor="@color/blue"
            android:textStyle="bold"
            android:textSize="11sp">
        </com.google.android.material.button.MaterialButton>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/claim"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginHorizontal="8dp"
            android:backgroundTint="#E9C96E"
            app:cornerRadius="10dp"
            android:text="Claim"
            android:fontFamily="@font/poppinsmedium"
            android:textColor="#404244"
            android:textStyle="bold"
            android:textSize="11sp"
            app:strokeColor="@color/blue"
            app:strokeWidth="1.3dp">
        </com.google.android.material.button.MaterialButton>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/setClaimedLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:visibility="gone">
    <com.google.android.material.button.MaterialButton
        android:id="@+id/setClaimed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="8dp"
        android:backgroundTint="#2F2F2F"
        app:cornerRadius="10dp"
        android:text="CLAIMED"
        android:fontFamily="@font/poppinsmedium"
        android:textStyle="bold"
        android:textSize="11sp">
    </com.google.android.material.button.MaterialButton>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/scan_layout_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:layout_marginBottom="10dp"
        android:visibility="gone">
        <com.google.android.material.button.MaterialButton
            android:id="@+id/scan_button_profile"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="8dp"
            android:backgroundTint="#E9C96E"
            app:cornerRadius="10dp"
            android:text="Scan Now"
            android:fontFamily="@font/poppinsmedium"
            android:textColor="#404244"
            android:textStyle="bold"
            android:textSize="11sp"
            app:strokeColor="@color/blue"
            app:strokeWidth="1.3dp">
        </com.google.android.material.button.MaterialButton>

    </LinearLayout>
    </RelativeLayout>
